    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-oxlint": "^1.30.0",
    "eslint-plugin-vue": "^10.6.2",
    "jiti": "^2.6.1",
    "less": "^4.4.2",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.30.0",
    "prettier": "^3.7.3",
    "rc-upload": "^4.11.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-tsc": "^3.1.5"
